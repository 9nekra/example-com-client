{
    "name": "9nekra/example-com-client",
    "type": "library",
    "description": "Simple client for example.com service.",
    "keywords": [
        "9nekra",
        "example-com-client"
    ],
    "homepage": "https://github.com/9nekra/example-com-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander",
            "email": "9nekra@gmail.com",
            "homepage": "https://github.com/9nekra",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nekra\\ExampleComClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nekra\\ExampleComClient\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
